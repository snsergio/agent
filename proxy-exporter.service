# Ubuntu services - proxy server - PUB-9aac242-20230810180222
[Unit]
Description=Start the Metrics Collector v5 proxy server

[Service]
Type=simple
User=monitor
Restart=always
RestartSec=10
CPUAccounting=yes
MemoryAccounting=yes

ExecStart=/home/monitor/monitor/pushprox/pushprox-proxy --web.listen-address=":9081"

[Install]
WantedBy=multi-user.target

